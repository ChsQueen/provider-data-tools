{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "definitions": {

    "Meta": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "versionId": {
          "type": "string"
        },
        "lastUpdated": {
          "type": "string"
        },
        "profile": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          }
        },
        "security": {
          "type": "array",
          "items": {
            "coding": {
              "$ref": "#/definitions/Coding"
            }
          }
        },
        "tag": {
          "type": "array",
          "items": {
            "coding": {
              "$ref": "#/definitions/Coding"
            }
          }
        }
        }
      },
    "Coding": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "system": {
          "type": "string",
          "format": "uri"
        },
        "version": {
          "type": "string"
        },
        "code": {
          "type": "string"
        },
        "display": {
          "type": "string"
        },
        "userSelected": {
          "type": "boolean"
        }
      }
      },
    "Resource": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "meta": {
          "ref": "#/definitions/Meta"
        },
        "implicitRules": {
          "type": "string",
          "format": "uri"
        },
        "language": {
          "type": "string"
        }
      }
    },
    "HumanName": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "resourceType": {
            "type":"string",
            "pattern": "^HumanName$"
          },
          "use": {
            "type": "string",
            "enum": ["usual" , "official" , "temp" , "nickname" , "anonymous" , "old" , "maiden"]
          },
          "text": {
            "type": "string"
          },
          "family": {
            "type": "array",
            "items": {
              "type": "string"
              }
            },
          "given": {
            "type":"array",
            "items": {
              "type": "string"
              }
            },
          "prefix": {
            "type":"array",
            "items": {
              "type": "string"
              }
            },
          "suffix": {
            "type":"array",
            "items": {
              "type": "string"
              }
          },
          "Period": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "start": {
                "type": "string",
                "format": "date-time"
              },
              "end": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
      },
    "CodeableConcept": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "coding": {
          "type": "array",
          "items": {
              "$ref": "#/definitions/Coding"
            }
          },
        "text": {
          "type": "string"
        }
      }
    },
    "Attatchment": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "contentType": {
          "type": "string"
        },
        "language": {
          "type": "string"
        },
        "data": {
          "type":"string"
        },
        "url": {
          "type":"string",
          "format": "uri"
        },
        "size": {
          "type": "integer",
          "minimum": 0
        },
        "hash": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "creation":{
          "type": "string",
          "format": "date-time"
        }
      }

    },
    "Reference": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "reference": {
          "type": "string"
        },
        "display": {
          "type": "string"
        }
      }
    },
    "Identifier": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "use": {
          "type": "string",
          "enum": ["usual", "official", "temp", "secondary"]
        },
        "type": {
          "$ref": "#/definitions/CodeableConcept"
        },
        "system": {
          "type": "string",
          "format": "uri"
        },
        "value": {
          "type": "string"
        },
        "period": {
          "$ref": "#/definitions/HumanName/Period"
        },
        "assigner": {
              "$ref": "#/definitions/Reference"
        }
      }
    },
    "Quantity": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "value":{
          "type": "number"
        },
        "comparator": {
          "type": "string"
        },
        "unit": {
          "type": "string"
        },
        "system": {
          "type": "string",
          "format": "uri"
        },
        "code": {
          "type": "string"
        }
      }
    },
    "SimpleQuantity": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "value":{
          "type": "number"
        },
        "unit": {
          "type": "string"
        },
        "system": {
          "type": "string",
          "format": "uri"
        },
        "code": {
          "type": "string"
        }
      }
    },
    "Range": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "low": {
          "$ref": "#/definitions/SimpleQuantity"
        },
        "high": {
          "$ref": "#/definitions/SimpleQuantity"
        }
      }
    },
    "Ratio": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "numerator": {
          "$ref": "#/definitions/Quantity"
        },
        "denominator": {
          "$ref": "#/definitions/Quantity"
        }
      }
    },
    "Address": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "resourceType":{
          "type": "string",
          "pattern": "^Address$"
        },
        "use": {
          "type": "string",
          "enum": ["home", "work", "temp", "old"]
        },
        "type": {
          "type": "string",
          "enum":["postal", "physical", "both"]
        },
        "text": {
          "type": "string"
        },
        "line": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "city": {
          "type": "string"
        },
        "district": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postalCode": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "period": {
          "$ref": "#definitions/HumanName/Period"
        }
      }
    },
    "ContactPoint": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "resourceType": {
          "type": "string",
          "pattern": "^ContactPoint$"
        },
        "system": {
          "type": "string",
          "enum": ["phone", "fax", "email", "paper", "other"]
        },
        "value": {
          "type": "string"
        },
        "use": {
          "type": "string",
          "enum": ["home", "work", "temp", "old", "mobile"]
        },
        "rank": {
          "type": "integer",
          "minimum": 1
        },
        "period": {
          "$ref": "#/definitions/HumanName/Period"
        }
      }
    },
    "Active": {
      "type": "boolean"
    },
    "Comment": {
      "type": "string"
    }
  },
  "type": "object",
  "additionalProperties": false,
  "required": ["actor"],
  "properties": {
    "resourceType": {
      "type":"string",
      "pattern": "^Schedule$"
    },
    "Identifier": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Identifier"
      }
    },
    "serviceCategory": {
      "$ref": "#/definitions/CodeableConcept"
    },
    "specialty": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CodeableConcept"
      }
    },
    "actor": {
      "$ref": "#/definitions/Reference"
    },
    "planningHorizon": {
      "$ref": "#/definitions/HumanName/Period"
    },
    "comment": {
      "$ref": "#/definitions/Comment"
    }
  }
}
